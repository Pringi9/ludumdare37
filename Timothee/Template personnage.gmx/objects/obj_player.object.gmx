{
    _id = instance_place(x, y, Iweapon)
    if (_id != noone)
    {
        sprite_index = _id.appearance;
        if (weapon != 0)
        {
            proj = instance_create(x, y, weapon_proj);
            proj.sprite_index = weapon_app;
        }
        weapon = _id.weapon;
        weapon_app = _id.sprite_index;
        mun = _id.mun;
        with(_id)
        {
            instance_destroy();
            audio_play_sound(snd_weapon_trhow, 5, false);
        }
    }
}
